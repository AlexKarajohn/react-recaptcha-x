{
  "name": "react-recaptcha-v3-v2",
  "version": "1.0.0",
  "description": "a React component for Google's reCAPTCHA v3 with v2 fallback",
  "main": "./dist/index.js",
  "scripts": {
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "webpack --config ./webpack.config.js --env.NODE_ENV=production --env.production --progress"
  },
  "keywords": [
    "react",
    "react recaptcha",
    "recaptcha v3",
    "recaptcha v2",
    "react recaptcha v3",
    "react recaptcha v2"
  ],
  "author": "Antonios Karagiannis",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "babel-loader": "^8.0.6",
    "babel-preset-minify": "^0.5.0",
    "react-dom": "^16.8.6",
    "tslint": "^5.16.0",
    "tslint-consistent-codestyle": "^1.15.1",
    "tslint-microsoft-contrib": "^6.2.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.1",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2"
  },
  "dependencies": {
    "@types/react": "^16.8.19",
    "react": "^16.8.6"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              "> 5%",
              "last 2 versions"
            ]
          }
        }
      ],
      "@babel/preset-react",
      "@babel/preset-typescript",
      "minify"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-react-jsx"
    ]
  },
  "prettier": {
    "singleQuote": true
  }
}
