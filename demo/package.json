{
  "name": "react-recaptcha-v3-v2-demo",
  "version": "1.0.0",
  "description": "demo of using the React component for Google's reCAPTCHA v3 with v2 fallback",
  "private": true,
  "scripts": {
    "start": "node --max-old-space-size=2048 node_modules/webpack-dev-server/bin/webpack-dev-server.js --config ./webpack.config.js",
    "build": "babel src/index.tsx --out-file dist/index.js"
  },
  "author": "Antonios Karagiannis",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "babel-loader": "^8.0.5",
    "babel-preset-minify": "^0.5.0",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "tslint": "^5.14.0",
    "tslint-microsoft-contrib": "^6.1.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.0"
  },
  "dependencies": {
    "@types/react": "^16.8.10",
    "@types/react-dom": "^16.8.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              "> 5%",
              "last 2 versions"
            ]
          }
        }
      ],
      "@babel/preset-react",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "prettier": {
    "singleQuote": true
  }
}
