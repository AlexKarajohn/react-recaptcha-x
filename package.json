    "build": "babel src/index.tsx --out-file dist/index.js"
  },
  "keywords": [
    "react",
    "react",
    "recaptcha",
    "recaptcha",
    "v3",
    "recaptcha",
    "v2",
    "react",
    "recaptcha",
    "v3"
  ],
  "author": "Antonios Karagiannis",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "babel-preset-minify": "^0.5.0"
  },
  "dependencies": {},
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
